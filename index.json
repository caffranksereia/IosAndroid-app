{
	"expo": {
		"name": "IosAndroid-app",
		"slug": "IosAndroid-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "src/_assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": ".src/_assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "src/_assets/apps/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "src/_assets/web/favicon.png"
		},
		"packagerOpts": {
			"sourceExts": ["js", "json", "ts", "tsx", "jsx"]
		}
	}
}
